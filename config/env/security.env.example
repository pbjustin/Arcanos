# Security and access controls

# Admin/registration controls
# ADMIN_KEY=your-admin-key
# REGISTER_KEY=your-register-key

# Optional root override for local development only
ALLOW_ROOT_OVERRIDE=false
# ROOT_OVERRIDE_TOKEN=replace-me

# Automation bypass for confirmation gate
# ARCANOS_AUTOMATION_SECRET=replace-me
# ARCANOS_AUTOMATION_HEADER=x-arcanos-automation

# Trusted GPT IDs for confirmation bypass
# TRUSTED_GPT_IDS=gpt-abc,gpt-def

# Daemon and registry rate limiting
# DAEMON_RATE_LIMIT_MAX=400
# DAEMON_RATE_LIMIT_WINDOW_MS=600000
# DAEMON_REGISTRY_RATE_LIMIT_MAX=120
# DAEMON_REGISTRY_RATE_LIMIT_WINDOW_MS=600000

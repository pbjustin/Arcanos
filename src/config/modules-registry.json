{
  "registry_metadata": {
    "name": "ARCANOS Module Registry",
    "version": "1.0.0",
    "description": "Backend registry for ARCANOS modules with OpenAI SDK compatibility",
    "created": "2025-01-26",
    "last_updated": "2025-01-26",
    "audit_enabled": true,
    "sdk_compatibility": "OpenAI SDK v5.15.0"
  },
  "modules": {
    "ARCANOS:GITHUB": {
      "metadata": {
        "name": "ARCANOS:GITHUB",
        "display_name": "GitHub Integration Module",
        "version": "1.0.0",
        "description": "Provides GitHub repository management and collaboration features",
        "provider": "GitHub REST API v3",
        "category": "integration",
        "status": "active",
        "created": "2025-01-26",
        "last_updated": "2025-01-26",
        "author": "ARCANOS System",
        "tags": ["github", "repository", "collaboration", "git"]
      },
      "requirements": {
        "environment_variables": [
          {
            "name": "GITHUB_TOKEN",
            "required": true,
            "description": "GitHub Personal Access Token for API authentication",
            "validation_pattern": "^(ghp_|gho_|ghu_|ghs_|ghr_)[a-zA-Z0-9]{36}$"
          },
          {
            "name": "GITHUB_USER",
            "required": true,
            "description": "GitHub username or organization name",
            "validation_pattern": "^[a-zA-Z0-9]([a-zA-Z0-9-]*[a-zA-Z0-9])?$"
          }
        ],
        "dependencies": [
          "openai@^5.15.0",
          "node-fetch@^3.0.0"
        ],
        "permissions": [
          "repo",
          "user:read",
          "pull_request:write"
        ]
      },
      "actions": {
        "createRepo": {
          "description": "Create a new GitHub repository",
          "method": "POST",
          "endpoint": "/repos",
          "openai_function": {
            "name": "github_create_repo",
            "description": "Create a new GitHub repository with specified configuration",
            "parameters": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string",
                  "description": "Repository name"
                },
                "description": {
                  "type": "string",
                  "description": "Repository description"
                },
                "private": {
                  "type": "boolean",
                  "description": "Whether the repository should be private",
                  "default": false
                },
                "auto_init": {
                  "type": "boolean",
                  "description": "Whether to initialize with README",
                  "default": true
                }
              },
              "required": ["name"]
            }
          },
          "audit_log": true,
          "rate_limit": {
            "requests_per_hour": 1000
          }
        },
        "commitFile": {
          "description": "Commit a file to a GitHub repository",
          "method": "PUT",
          "endpoint": "/repos/{owner}/{repo}/contents/{path}",
          "openai_function": {
            "name": "github_commit_file",
            "description": "Commit a file to a GitHub repository",
            "parameters": {
              "type": "object",
              "properties": {
                "owner": {
                  "type": "string",
                  "description": "Repository owner (username or organization)"
                },
                "repo": {
                  "type": "string",
                  "description": "Repository name"
                },
                "path": {
                  "type": "string",
                  "description": "File path in the repository"
                },
                "content": {
                  "type": "string",
                  "description": "Base64-encoded file content"
                },
                "message": {
                  "type": "string",
                  "description": "Commit message"
                },
                "branch": {
                  "type": "string",
                  "description": "Branch name",
                  "default": "main"
                }
              },
              "required": ["owner", "repo", "path", "content", "message"]
            }
          },
          "audit_log": true,
          "rate_limit": {
            "requests_per_hour": 5000
          }
        },
        "openPR": {
          "description": "Open a new pull request",
          "method": "POST",
          "endpoint": "/repos/{owner}/{repo}/pulls",
          "openai_function": {
            "name": "github_open_pr",
            "description": "Open a new pull request in a GitHub repository",
            "parameters": {
              "type": "object",
              "properties": {
                "owner": {
                  "type": "string",
                  "description": "Repository owner (username or organization)"
                },
                "repo": {
                  "type": "string",
                  "description": "Repository name"
                },
                "title": {
                  "type": "string",
                  "description": "Pull request title"
                },
                "body": {
                  "type": "string",
                  "description": "Pull request description"
                },
                "head": {
                  "type": "string",
                  "description": "The name of the branch where your changes are implemented"
                },
                "base": {
                  "type": "string",
                  "description": "The name of the branch you want the changes pulled into",
                  "default": "main"
                },
                "draft": {
                  "type": "boolean",
                  "description": "Whether to create as draft PR",
                  "default": false
                }
              },
              "required": ["owner", "repo", "title", "head"]
            }
          },
          "audit_log": true,
          "rate_limit": {
            "requests_per_hour": 1000
          }
        },
        "listIssues": {
          "description": "List issues in a GitHub repository",
          "method": "GET",
          "endpoint": "/repos/{owner}/{repo}/issues",
          "openai_function": {
            "name": "github_list_issues",
            "description": "List issues in a GitHub repository",
            "parameters": {
              "type": "object",
              "properties": {
                "owner": {
                  "type": "string",
                  "description": "Repository owner (username or organization)"
                },
                "repo": {
                  "type": "string",
                  "description": "Repository name"
                },
                "state": {
                  "type": "string",
                  "enum": ["open", "closed", "all"],
                  "description": "Issue state filter",
                  "default": "open"
                },
                "labels": {
                  "type": "string",
                  "description": "Comma-separated list of label names to filter by"
                },
                "sort": {
                  "type": "string",
                  "enum": ["created", "updated", "comments"],
                  "description": "Sort field",
                  "default": "created"
                },
                "direction": {
                  "type": "string",
                  "enum": ["asc", "desc"],
                  "description": "Sort direction",
                  "default": "desc"
                },
                "per_page": {
                  "type": "integer",
                  "minimum": 1,
                  "maximum": 100,
                  "description": "Number of results per page",
                  "default": 30
                }
              },
              "required": ["owner", "repo"]
            }
          },
          "audit_log": false,
          "rate_limit": {
            "requests_per_hour": 5000
          }
        }
      },
      "audit_configuration": {
        "enabled": true,
        "trace_requests": true,
        "log_responses": true,
        "retention_days": 90,
        "include_metadata": true,
        "sanitize_tokens": true,
        "audit_fields": [
          "action",
          "timestamp",
          "user_id",
          "request_id",
          "parameters",
          "response_status",
          "rate_limit_remaining"
        ]
      },
      "versioning": {
        "current_version": "1.0.0",
        "api_version": "v3",
        "compatibility": {
          "min_arcanos_version": "1.4.0",
          "max_arcanos_version": "2.0.0"
        },
        "changelog": [
          {
            "version": "1.0.0",
            "date": "2025-01-26",
            "changes": [
              "Initial implementation",
              "Added core GitHub actions",
              "OpenAI SDK compatibility",
              "Audit tracing support"
            ]
          }
        ]
      },
      "security": {
        "authentication_method": "token",
        "token_validation": true,
        "rate_limiting": true,
        "request_signing": false,
        "allowed_origins": ["github.com", "api.github.com"],
        "timeout_ms": 30000
      },
      "monitoring": {
        "health_check_endpoint": "/repos/{owner}/{repo}",
        "metrics_enabled": true,
        "performance_tracking": true,
        "error_reporting": true
      }
    }
  }
}